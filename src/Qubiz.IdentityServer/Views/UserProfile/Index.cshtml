@model IEnumerable<Qubiz.IdentityServer.Models.UserProfile>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Company)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BirthDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mobile)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SkypeID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ContractStartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkStartDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CarModel)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CarRegistration)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TShirtSize)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IDIssueDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IDExpiryDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PassIssueDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PassExpiryDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Company)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BirthDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Mobile)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SkypeID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ContractStartDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WorkStartDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CarModel)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CarRegistration)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TShirtSize)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IDIssueDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IDExpiryDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PassIssueDate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PassExpiryDate)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a><br />
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a><br />
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
